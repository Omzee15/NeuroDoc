# Copy this file to .env.local and fill in your actual API keys

# PDF Chat API Key (get from https://aistudio.google.com/app/apikey)
VITE_PDF_CHAT_GEMINI_API=your_pdf_chat_api_key_here

# Podcast Generation API Keys (dual keys for better rate limiting)
VITE_PODCAST_GEMINI_API=your_podcast_api_key_1_here
VITE_PODCAST_GEMINI_API_2=your_podcast_api_key_2_here

# Optional: Hugging Face API Key
VITE_HUGGINGFACE_API=your_huggingface_api_key_here

# Optional: Google Web Risk API Key
VITE_WEBRISK_API=your_webrisk_api_key_here